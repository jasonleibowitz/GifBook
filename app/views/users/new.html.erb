<%= form_for @user do |f|%>

  <div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  </div>

  <div>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  </div>

  <div>
  <%= f.label :city %>
  <%= f.text_field :city %>
  </div>

  <div>
  <%= f.label :state %>
  <%= f.text_field :state %>
  </div>

  <div>
  <%= f.label :avatar_url %>
  <%= f.text_field :avatar_url %>
  </div>

  <div>
  <%= f.label :email %>
  <%= f.email_field :email %>
  </div>

  <div>
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div>

  <div>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  </div>

  <%= f.submit %>


<% end %>
